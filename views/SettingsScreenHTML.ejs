<!DOCTYPE html>
<html>

<head>
  <%- include('partials/head'); %>

  <style type="text/css">
    body {
			font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
			margin: auto;
			max-width: 38rem;
			background-color: #fffafa;
			text-align: center;
			max-width: 100%;
		}

    #gobacklink {
      font-size: 25px;
      text-decoration: none;
      color: black;
      position: absolute;
      left: 15px;
      top: 15px;
    }

    .sett {
      background-color: #d1d1d1;
      color: black;
      font-size: 25px;
      padding: 30px;
      margin: 150px 100px 2px 80px;
      border-radius: 20px;
      border: none;
      width: 80%;
      transition-duration: 0.4s;
    }

    .sett:hover {
      background-color: black;
      color: white;
    }

    .settb {
      margin-top: 15px;
    }
  </style>

</head>

<body>
  <div class="sidePanel">
    <%- include('partials/sidePanel'); %>
  </div>
  <div class="mainPanel">
    <a href="javascript:history.back()" id="gobacklink"><i class='bx bx-log-out-circle' ></i></i>&nbsp;&nbsp;Go back</a>
    <div class="settb">
      <table>
        <tr>
          <button class="sett" onclick="window.location.href='/AccountSettingsHTML'"><i class="fa fa-gear"
              style="font-size:28px; font-family:Helvetica;">Account Settings</button></i>
          <button class="sett" onclick="window.location.href='/AboutHTML'"><i class="fas fa-edit"
              style="font-size:28px; font-family:Helvetica;">About Intellignosis</button></i>
        </tr>

      </table>
    </div>
    <%- include('partials/chatbot'); %>

  </div>
</body>

</html>